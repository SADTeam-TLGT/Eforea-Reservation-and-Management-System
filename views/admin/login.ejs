<body>
	<div id="formWrapper">
		<div id="formHolder">
			<div id="form">
				<div class="logo">

				</div>
				<div class="form-item">
					<div class="row">
						<div class="col-md-6 left">
							<h1 class="logoname"> EForea </h1>
							<h2 class="logoname"> Nail and Body Spa</h2>
							<hr>
							<center>
								<img class="img-responsive logodesign" width="100" height="50" src="assets/images/icon-lotus.png" alt="Lotus-image">
							</center>
							<br><br><br>
						</div>
						<div class="col-md-6 right">
							<form id="credential" action="#" method="post">
								<div class="form-item">
								<p class="formLabel">Username</p>
									<input type="text" name="username" id="username" class="form-style" autocomplete="off"/>
								</div>
								<div class="form-item">
									<p class="formLabel">Password</p>
									<input type="password" name="password" id="password" class="form-style" />
									<br />
									<p><a href="#" ><small>Forgot Password</small></a></p>	
								</div>
								<div class="form-item">
									<input type="submit" class="login pull-right" value="Log In">
								<div class="clear-fix"></div>
								</div>
							</form>
						</div>
					</div>
					<div>
						<p class="copyright"> &copy; Copyright 2018 </p>
					</div>
			</div>
		</div>
		
	</div>
	<script src="assets/js/sweetalert.min.js"></script>
